{
  "version": "0.0.7",
  "license": "MIT",
  "description": "Helpers library for my app",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/src/index.d.ts",
  "engines": {
    "node": ">=20"
  },
  "files": ["build"],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && tsc --outDir build",
    "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --out-dir build/cjs --source-maps",
    "build:esm": "NODE_ENV=production BABEL_ENV=esm babel src --out-dir build/esm --source-maps",
    "prepublishOnly": "npm run build"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [],
  "name": "@meiti-x/myapp-shared",
  "author": "Meiti",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meiti-x/jobber-shared.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    },
    "./icons": {
      "import": "./build/esm/icons/index.js",
      "require": "./build/cjs/icons/index.js"
    }
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.2",
    "cloudinary": "^2.6.1",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9"
  }
}
